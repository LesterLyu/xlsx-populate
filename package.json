    "cfb": "^1.1.0",
    "fast-formula-parser": "git+https://github.com/LesterLyu/fast-formula-parser.git",
    "jszip": "^3.2.1",
    "lodash": "^4.17.11",
    "saxes": "^3.1.9"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dtjohnson/xlsx-populate.git"
  },
  "bugs": "https://github.com/dtjohnson/xlsx-populate/issues",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babelify": "^8.0.0",
    "browserify": "^16.2.3",
