    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify": "^3.0.2",
    "jasmine": "^3.4.0",
    "jsdoc-to-markdown": "^4.0.1",
    "jsondiffpatch": "^0.3.11",
    "karma": "^2.0.5",
